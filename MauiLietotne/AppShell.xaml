<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiLietotne.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiLietotne"
    xmlns:forms="clr-namespace:MauiLietotne.Forms"
    Title="MauiLietotne">

    <ShellContent
        Title="Data Managment Page"
        ContentTemplate="{DataTemplate forms:DataManagmentPage}"
        Route="DataManagmentPage" />
    <ShellContent
        Title="Main Test Page"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>

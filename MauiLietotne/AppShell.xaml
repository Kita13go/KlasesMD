<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiLietotne.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiLietotne"
    Title="MauiLietotne">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Title"
        ContentTemplate="{DataTemplate local:TitlePage}"
        Route="Title" />
    <ShellContent
        Title="Data"
        ContentTemplate="{DataTemplate local:DataManagmentPage}"
        Route="DataManagmentPage" />


</Shell>
